<head>
  <link rel="stylesheet" href="/main.css">
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.98.2/css/materialize.min.css">
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.98.2/css/materialize.min.css">
  <link href="http://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
  <link type="text/css" rel="stylesheet" href="/materialize.min.css"  media="screen,projection"/>
  <meta name="viewport" content="width=device-width, initial-scale=1.0"/>

</head>
<body>
  <header class="container">
     <h1>Reviews</h1>
  </header>
  <nav  class="container pink lighten-2">
    <div class="nav-wrapper">
      <ul id="nav-mobile " class" hide-on-med-and-down">
        <li><a class="pulse" href='/'>Home</a></li>
        <li><a class="pulse" href='/courses'>Courses</a></li>
        <li><a class="pulse" href='/contact'>Contact Us </a></li>
      </ul>
    </div>
  </nav>

     <% reviews.rev.forEach( function(review) { %>
       <div class="container">
         <p class="reviewItem"><%= review %></p>
       </div>
     <% }); %>

    <div class="container right-align ">
      <div class="backBtn">
        <a class="btn blue lighten-3 " href=<%= '/courses/' + reviews.id %> >Back</a>
      </div>
   </div>
  <script type="text/javascript" src="https://code.jquery.com/jquery-2.1.1.min.js"></script>
  <script type="text/javascript" src="/materialize.min.js"></script>
</body>
